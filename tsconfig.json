{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"strict": true,
		"importHelpers": true,
		"outDir": "build",
		"noUncheckedIndexedAccess": true
	},
	"exclude": ["node_modules"]
}
